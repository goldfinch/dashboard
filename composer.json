{
    "name": "goldfinch/dashboard",
    "description": "Extendable Dashboard for Silverstripe",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "silverstripe-dashboard",
        "dashboard",
        "enchantment-dashboard",
        "silverstripe-admin-dashboard",
        "admin-dashboard"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Artem Karasev"
        }
    ],
    "require": {
        "php": ">=8.0",
        "silverstripe/framework": "^5.0",
        "silverstripe/admin": "^2.0",
        "goldfinch/dashcore": "^1.0",
        "goldfinch/taz": "^1.0",
        "goldfinch/helpers": "^1.0",
        "goldfinch/extra-assets": "^1.0",
        "bitbucket/client": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Goldfinch\\Dashboard\\": "src/"
        }
    },
    "extra": {
        "expose": ["client/dist"]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
